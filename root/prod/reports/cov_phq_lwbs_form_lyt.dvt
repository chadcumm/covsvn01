WITH NOCOUNTER, SEPARATOR=" ", FORMAT]]></RPTQUERYSTRING><RPTSELECTIF condition="1"><![CDATA[]]></RPTSELECTIF><RPTSELECTIF condition="0"><![CDATA[]]></RPTSELECTIF><RPTSELECTIF condition="0"><![CDATA[]]></RPTSELECTIF></RPTQUERY><RPTDRIVER name="COV_PHQ_LWBS_FORM" params="&quot;NL:&quot;, $start_datetime, $end_datetime, $facility"/><RPTRECORD><![CDATA[record lwbs (
