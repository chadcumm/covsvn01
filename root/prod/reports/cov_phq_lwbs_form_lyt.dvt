	PLIST_FACILITY = SUBSTRING(1, 30, LWBS->plist[D1.SEQ].facility)
	, PLIST_ED_GROUP = SUBSTRING(1, 30, LWBS->plist[D1.SEQ].ed_group)
	, PLIST_ENCNTRID = LWBS->plist[D1.SEQ].encntrid
	, PLIST_DISPO_CD = LWBS->plist[D1.SEQ].dispo_cd
	, PLIST_DISPOSITION = SUBSTRING(1, 30, LWBS->plist[D1.SEQ].disposition)
	, PLIST_CHKOUT_DISPO_CD = LWBS->plist[D1.SEQ].chkout_dispo_cd

FROM
