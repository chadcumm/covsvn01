	, UNIT_NU_TOT_COMP_ADMIN = BCMA_TOT->unit[D1.SEQ].nu_tot_comp_admin
	, BCMA_TOT_MONTH_SELECTED = SUBSTRING(1, 30, BCMA_TOT->month_selected)
	, BCMA_TOT_YEAR_SELECTED = BCMA_TOT->year_selected
	, BCMA_TOT_FAC_TOT_LEAP_ADMIN = BCMA_TOT->fac_tot_leap_admin
	, UNIT_LEAPFROG_UNIT = SUBSTRING(1, 30, BCMA_TOT->unit[D1.SEQ].leapfrog_unit)
	, UNIT_LP_NU_TOT_ADMIN = BCMA_TOT->unit[D1.SEQ].lp_nu_tot_admin
	, UNIT_LP_NU_TOT_COMP_ADMIN = BCMA_TOT->unit[D1.SEQ].lp_nu_tot_comp_admin

FROM
	(DUMMYT   D1  WITH SEQ = SIZE(BCMA_TOT->unit, 5))

PLAN D1

ORDER BY
	BCMA_TOT_FACILITY
	, UNIT_NURSE_UNIT

