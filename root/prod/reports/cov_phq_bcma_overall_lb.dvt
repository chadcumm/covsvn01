	BCMA_TOT_FACILITY = SUBSTRING(1, 30, BCMA_TOT->facility)
	, BCMA_TOT_FAC_TOT_ADMIN = BCMA_TOT->fac_tot_admin
	, BCMA_TOT_FAC_TOT_COMP_ADMIN = BCMA_TOT->fac_tot_comp_admin
	, BCMA_TOT_FAC_TOT_LEAP_COMP_ADMIN = BCMA_TOT->fac_tot_leap_comp_admin
	, UNIT_NURSE_UNIT = SUBSTRING(1, 30, BCMA_TOT->unit[D1.SEQ].nurse_unit)
