	, PCOMP_ORDER_SENTENCE = SURG_SCHED->qual[D1.SEQ].pw[D3.SEQ].pcomp[D4.SEQ].order_sentence

FROM
	(DUMMYT   D1  WITH SEQ = VALUE(SURG_SCHED->cnt))
	, (DUMMYT   D2  WITH SEQ = 1)
	, (DUMMYT   D3  WITH SEQ = 1)
	, (DUMMYT   D4  WITH SEQ = 1)

PLAN D1
 WHERE MAXREC(D2, SURG_SCHED->qual[D1.SEQ].proc_cnt)

