<property name = 'DEFAULT-VALUES:00'><![CDATA[MULTI-DEF-COUNT='0'                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     ]]></property>
<property name = 'SETTINGS:00'><![CDATA[DEFAULT-KEY='' QUERY-STRING='
SELECT
    IF ($dta &#061; 1.0 AND $sr&#061;1.0)
    DISTINCT
    PLAN A
    WHERE a.service_resource_cd > 0
    AND a.task_assay_cd > 0
    JOIN C
    WHERE A.LOT_ID &#061; C.LOT_ID
    ORDER BY C.LOT_NUMBER 
    ELSEIF($dta &#061; 1.0 and $sr > 1.0)
    DISTINCT
    PLAN A
    WHERE a.service_resource_cd &#061; $sr
    AND a.task_assay_cd > 0
    JOIN C
    WHERE A.LOT_ID &#061; C.LOT_ID
    ORDER BY C.LOT_NUMBER 
    ELSEIF ($dta > 1.0)
    DISTINCT
    PLAN a
    WHERE a.service_resource_cd &#061; $sr
    AND a.task_assay_cd &#061; $dta
    JOIN c
    WHERE a.lot_id &#061; c.lot_id
    ORDER BY c.lot_number
    ENDIF
    
    C.LOT_NUMBER,
    C.lot_id

FROM
    ASSAY_RESOURCE_LOT   A
    , CONTROL_LOT   C

PLAN A
join C
WHERE A.LOT_ID &#061; C.LOT_ID' QUERY-OPTIONS='1067' COLUMNS='&#123;TABLE&#061;&#034;&#034; ORDINAL&#061;&#034;0&#034; VISIBLE&#061;&#034;true&#034; COLUMN&#061;&#034;0&#034; ALIAS&#061;&#034;&#034;]]></property>
