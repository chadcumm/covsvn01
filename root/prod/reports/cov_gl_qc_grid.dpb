<property name = 'SETTINGS:00'><![CDATA[DEFAULT-KEY='' QUERY-STRING='SELECT IF ($sr &#061; 1)
        DISTINCT
        WHERE apr.service_resource_cd IN (SELECT sr.service_resource_cd
        FROM service_resource sr WHERE sr.location_cd &#061; $perflab )
        AND apr.task_assay_cd > 0.0
        ELSEIF ($sr > 1)
        DISTINCT
        WHERE apr.service_resource_cd &#061; $sr
        ENDIF
    
    dta &#061; UAR_GET_CODE_DISPLAY(apr.task_assay_cd),
    apr.task_assay_cd

FROM assay_processing_r apr
WHERE apr.active_ind &#061; 1
AND apr.task_assay_cd > 0.00

ORDER BY dta' QUERY-OPTIONS='1067' COLUMNS='&#123;TABLE&#061;&#034;&#034; ORDINAL&#061;&#034;0&#034; VISIBLE&#061;&#034;true&#034; COLUMN&#061;&#034;0&#034; ALIAS&#061;&#034;&#034; FORMAT&#061;&#034;&#034; WIDTH&#061;&#034;42.000000&#034; TYPE&#061;&#034;&#034; VALUE&#061;&#034;false&#034; BINDING&#061;&#034;DTA&#034; TITLE&#061;&#034;DTA&#034; &#125;&#123;TABLE&#061;&#034;&#034; ORDINAL&#061;&#034;1&#034; VISIBLE&#061;&#034;false&#034; COLUMN&#061;&#]]></property>
