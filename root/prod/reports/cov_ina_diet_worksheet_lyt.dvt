]]></RPTPROMPTS><RPTQUERY name="get_patients" layout="1"><RPTQUERYSTRING><![CDATA[SELECT
	DIET_RECCNT = DIET->reccnt
	, PLIST_FACILITY = DIET->plist[D1.SEQ].facility
	, PLIST_NURSE_UNIT = DIET->plist[D1.SEQ].nurse_unit
	, PLIST_ROOM = DIET->plist[D1.SEQ].room
	, PLIST_BED = DIET->plist[D1.SEQ].bed
	, PLIST_PAT_NAME = SUBSTRING(1, 30, DIET->plist[D1.SEQ].pat_name)
	, PLIST_PERSONID = DIET->plist[D1.SEQ].personid
	, PLIST_ENCNTRID = DIET->plist[D1.SEQ].encntrid
	, PLIST_MRN = SUBSTRING(1, 30, DIET->plist[D1.SEQ].mrn)
	, PLIST_FIN = SUBSTRING(1, 30, DIET->plist[D1.SEQ].fin)
	, PLIST_ADMITTING_MD = SUBSTRING(1, 30, DIET->plist[D1.SEQ].admitting_md)
	, PLIST_DOB = SUBSTRING(1, 30, DIET->plist[D1.SEQ].dob)
	, PLIST_AGE = SUBSTRING(1, 30, DIET->plist[D1.SEQ].age)
	, PLIST_GENDER = SUBSTRING(1, 30, DIET->plist[D1.SEQ].gender)
	, PLIST_HEIGHT = DIET->plist[D1.SEQ].height
	, PLIST_HIGHT_DATE = SUBSTRING(1, 30, DIET->plist[D1.SEQ].hight_date)
	, PLIST_WEIGHT = SUBSTRING(1, 30, DIET->plist[D1.SEQ].weight)
	, PLIST_WEIGHT_DATE = SUBSTRING(1, 30, DIET->plist[D1.SEQ].weight_date)
	, PLIST_ADMIT_DATE = SUBSTRING(1, 30, DIET->plist[D1.SEQ].admit_date)
	, PLIST_LOS = DIET->plist[D1.SEQ].los
	, PLIST_DX = SUBSTRING(1, 30, DIET->plist[D1.SEQ].dx)
	, PLIST_DIET = SUBSTRING(1, 100, DIET->plist[D1.SEQ].diet)
	, PLIST_DAYS_ON_DIET = DIET->plist[D1.SEQ].days_on_diet
	, PLIST_COMMENTS = SUBSTRING(1, 300, DIET->plist[D1.SEQ].comments)
	, PLIST_ALLERGIES = SUBSTRING(1, 300, DIET->plist[D1.SEQ].allergies)
	, PLIST_ORDERID = DIET->plist[D1.SEQ].orderid
	, PLIST_HEIGHT_CM = SUBSTRING(1, 30, DIET->plist[D1.SEQ].height_cm)

FROM
	(DUMMYT   D1  WITH SEQ = VALUE(SIZE(DIET->plist, 5)))

PLAN D1

ORDER BY
	PLIST_FACILITY
	, PLIST_NURSE_UNIT
	, PLIST_ROOM
	, PLIST_BED
	, PLIST_ENCNTRID

WITH NOCOUNTER, SEPARATOR=" ", FORMAT]]></RPTQUERYSTRING><RPTSELECTIF condition="1"><![CDATA[]]></RPTSELECTIF><RPTSELECTIF condition="0"><![CDATA[]]></RPTSELECTIF><RPTSELECTIF condition="0"><![CDATA[]]></RPTSELECTIF></RPTQUERY><RPTDRIVER name="cov_ina_dietitian_worksheet" params="&quot;NL:&quot;,  $facility_list"/><RPTRECORD><![CDATA[record diet (
  1 reccnt = i4   
  1 plist [*]  
    2 facility = f8   
    2 nurse_unit = f8   
    2 room = f8   
    2 bed = f8   
    2 pat_name = vc   
    2 personid = f8   
    2 encntrid = f8   
    2 mrn = vc   
    2 fin = vc   
    2 admitting_md = vc   
    2 dob = vc   
    2 age = vc   
    2 gender = vc   
    2 height = f8   
    2 hight_date = vc   
    2 weight = vc   
    2 weight_date = vc   
    2 admit_date = vc   
    2 los = i4   
    2 dx = vc   
    2 diet = vc   
    2 days_on_diet = i4   
    2 comments = vc   
    2 allergies = vc   
    2 orderid = f8   
    2 height_cm = vc   
) 
]]></RPTRECORD><RPTHEADERS><RPTHEADER name="Head Report" version="1" keeptogether="1" keepgroup="0" repeat="0" forcefootpage="0"/><RPTHEADER name="Head Page" version="1" keeptogether="1" keepgroup="0" repeat="0" forcefootpage="0"><RPTSECTION name="HeadPageSection"/></RPTHEADER><RPTHEADER name="Head PLIST_FACILITY" version="1" keeptogether="1" keepgroup="0" repeat="0" forcefootpage="0"><RPTSECTION name="HeadPLIST_FACILITYSection"/></RPTHEADER><RPTHEADER name="Head PLIST_NURSE_UNIT" version="1" keeptogether="1" keepgroup="0" repeat="0" forcefootpage="0"><RPTSECTION name="HeadPLIST_NURSE_UNITSection"/></RPTHEADER><RPTHEADER name="Head PLIST_ENCNTRID" version="1" keeptogether="1" keepgroup="0" repeat="0" forcefootpage="0"><RPTSECTION name="HeadPLIST_ENCNTRIDSection"/></RPTHEADER><RPTHEADER name="Detail" version="1" keeptogether="1" keepgroup="0" repeat="0" forcefootpage="0"><RPTSECTION name="DetailSection"/></RPTHEADER><RPTHEADER name="Foot PLIST_ENCNTRID" version="1" keeptogether="1" keepgroup="0" repeat="0" forcefootpage="0"><RPTSECTION name="FootPLIST_ENCNTRIDSection"/></RPTHEADER><RPTHEADER name="Foot PLIST_NURSE_UNIT" version="1" keeptogether="1" keepgroup="0" repeat="0" forcefootpage="0"><RPTSECTION name="FootPLIST_NURSE_UNITSection"/></RPTHEADER><RPTHEADER name="Foot PLIST_FACILITY" version="1" keeptogether="1" keepgroup="0" repeat="0" forcefootpage="0"><RPTSECTION name="FootPLIST_FACILITYSection"/></RPTHEADER><RPTHEADER name="Foot Page" version="1" keeptogether="1" keepgroup="0" repeat="0" forcefootpage="0"><RPTSECTION name="FootPageSection"/></RPTHEADER></RPTHEADERS></REPORTLAYOUT>
