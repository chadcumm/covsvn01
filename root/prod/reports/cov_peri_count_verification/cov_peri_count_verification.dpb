<property name = 'SETTINGS:00'><![CDATA[DEFAULT-KEY='' QUERY-STRING='select
     surgery_area &#061; uar_get_code_display(sr.service_resource_cd)
    ,surgery_area_cd &#061; sr.service_resource_cd
from
     PRSNL pr
    ,PRSNL_ORG_RELTN por
    ,ORGANIZATION org
    ,LOCATION l
    ,SERVICE_RESOURCE sr
    ,CODE_VALUE cv
plan  pr where pr.person_id&#061;reqinfo->updt_id
    and pr.active_ind &#061; 1
join por where pr.person_id&#061;por.person_id
    and por.end_effective_dt_tm > sysdate
    and por.active_ind &#061; 1
join org where org.organization_id &#061; por.organization_id
join l where org.organization_id &#061; l.organization_id
    and l.organization_id&#061; $facility_pmpt 
    and l.active_ind &#061; 1
    and l.location_type_cd &#061; 817.00    ;surgery
join sr where sr.location_cd&#061;l.location_cd
    and sr.active_ind&#061;1
join cv where cv.code_value&#061;sr.service_resource_cd
;and cv.display_key!&#061;"*pat*"
order by surgery_area
' QUERY-OPTIONS='1067' COLUMNS='&#123;TABLE&#061;&]]></property>
