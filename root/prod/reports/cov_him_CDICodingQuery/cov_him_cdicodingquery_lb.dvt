]]></RPTPROMPTS><RPTQUERY name="Query1" layout="1"><RPTQUERYSTRING><![CDATA[SELECT
	CDI_STARTDATE = SUBSTRING(1, 30, CDI->startdate)
	, CDI_ENDDATE = SUBSTRING(1, 30, CDI->enddate)
	, LIST_ENCNTR_ID = CDI->list[D1.SEQ].encntr_id
	, LIST_PERSON_ID = CDI->list[D1.SEQ].person_id
	, LIST_PATIENT_NAME = CDI->list[D1.SEQ].patient_name
	, LIST_FIN = CDI->list[D1.SEQ].fin
	, LIST_FACILITY = CDI->list[D1.SEQ].facility
	, LIST_NURSEUNIT = CDI->list[D1.SEQ].nurseunit
	, LIST_DX = CDI->list[D1.SEQ].dx
	, LIST_DISCH_DT_TM = CDI->list[D1.SEQ].disch_dt_tm
	, NRLIST_PARENT_EVENT_ID = CDI->list[D1.SEQ].nrlist[D2.SEQ].parent_event_id
	, NRLIST_EVENT_ID = CDI->list[D1.SEQ].nrlist[D2.SEQ].event_id
	, NRLIST_EVENT_TITLE = CDI->list[D1.SEQ].nrlist[D2.SEQ].event_title
	, NRLIST_EVENT_CD = CDI->list[D1.SEQ].nrlist[D2.SEQ].event_cd
	, NRLIST_EVENT_TYPE = CDI->list[D1.SEQ].nrlist[D2.SEQ].event_type
	, NRLIST_ACTION_TYPE = CDI->list[D1.SEQ].nrlist[D2.SEQ].action_type
	, NRLIST_ACTION_BY = CDI->list[D1.SEQ].nrlist[D2.SEQ].action_by
	, NRLIST_ACTION_DATE = CDI->list[D1.SEQ].nrlist[D2.SEQ].action_date
	, NRLIST_ACTION_DESC = CDI->list[D1.SEQ].nrlist[D2.SEQ].action_desc
	, NRLIST_RESULT_STATUS = CDI->list[D1.SEQ].nrlist[D2.SEQ].result_status
	, NRLIST_ADDENDUM = CDI->list[D1.SEQ].nrlist[D2.SEQ].addendum
	, NRLIST_ADDENDUM_BY = CDI->list[D1.SEQ].nrlist[D2.SEQ].addendum_by
	, NRLIST_ADDENDUM_DATE = CDI->list[D1.SEQ].nrlist[D2.SEQ].addendum_date
	, NRLIST_ADDENDUM_DESC = CDI->list[D1.SEQ].nrlist[D2.SEQ].addendum_desc

FROM
	(DUMMYT   D1  WITH SEQ = SIZE(CDI->list, 5))
	, (DUMMYT   D2  WITH SEQ = 1)

PLAN D1 WHERE MAXREC(D2, SIZE(CDI->list[D1.SEQ].nrlist, 5))
JOIN D2

ORDER BY
	LIST_FACILITY
	, LIST_NURSEUNIT
	, LIST_FIN
	, NRLIST_ACTION_DATE
	, NRLIST_PARENT_EVENT_ID
	, NRLIST_EVENT_ID

WITH NOCOUNTER, SEPARATOR=" ", FORMAT]]></RPTQUERYSTRING><RPTSELECTIF condition="1"><![CDATA[]]></RPTSELECTIF><RPTSELECTIF condition="0"><![CDATA[]]></RPTSELECTIF><RPTSELECTIF condition="0"><![CDATA[]]></RPTSELECTIF></RPTQUERY><RPTDRIVER name="cov_him_CDICodingQuery" params="$outdev, $facility, $start_datetime, $end_datetime"/><RPTCODEVALUE name="REMINDERS_VAR" type="DISPLAYKEY" expression="REMINDERS" codeset="72"/><RPTRECORD><![CDATA[record cdi (
  1 startdate = vc   
  1 enddate = vc   
  1 cnt = i4   
  1 list [*]  
    2 encntr_id = f8   
    2 person_id = f8   
    2 patient_name = c100   
    2 fin = c20   
    2 facility = c60   
    2 nurseunit = c40   
    2 dx = c400   
    2 disch_dt_tm = dq8   
    2 nrcnt = i4   
    2 nrlist [*]  
      3 parent_event_id = f8   
      3 event_id = f8   
      3 event_title = c100   
      3 event_cd = f8   
      3 event_type = c40   
      3 action_type = c40   
      3 action_by = c100   
      3 action_date = dq8   
      3 action_desc = c10   
      3 result_status = c40   
      3 addendum = c1024   
      3 addendum_by = c100   
      3 addendum_date = dq8   
      3 addendum_desc = c10   
      3 eid = f8   
      3 blob_seq_num = i4   
      3 blob_length = i4   
      3 valid_from_dt_tm = dq8   
      3 valid_until_dt_tm = dq8   
      3 updt_dt_tm = dq8   
) 
]]></RPTRECORD><RPTVAR name="PRINTED_DATETIME" type="vc" init="build2(&quot;Printed: &quot;, curdate, &quot; &quot;, curtime)" access="1" constant="1" expression="1"><RPTVARDECLARE><![CDATA[declare PRINTED_DATETIME = vc with Constant(build2("Printed: ", curdate, " ", curtime)),Protect]]></RPTVARDECLARE></RPTVAR><RPTHEADERS><RPTHEADER name="Head Page" version="1" keeptogether="1" keepgroup="0" repeat="0" forcefootpage="0"><RPTSECTION name="HeadPageSection"/></RPTHEADER><RPTHEADER name="Head LIST_FACILITY" version="1" keeptogether="1" keepgroup="1" repeat="1" forcefootpage="0"><RPTSECTION name="HeadLIST_FACILITYSection"/></RPTHEADER><RPTHEADER name="Head LIST_FIN" version="1" keeptogether="1" keepgroup="0" repeat="0" forcefootpage="0"><RPTSECTION name="HeadLIST_FINSection"/><RPTSECTION name="HeadLIST_FINSection1"/></RPTHEADER><RPTHEADER name="Detail" version="1" keeptogether="1" keepgroup="0" repeat="0" forcefootpage="0"><RPTSECTION name="DetailSection"/><RPTSECTION name="DetailSection1"/><RPTSECTION name="DetailSection2"/></RPTHEADER><RPTHEADER name="Foot LIST_FIN" version="1" keeptogether="1" keepgroup="0" repeat="0" forcefootpage="0"><RPTSECTION name="FootLIST_FINSection"/></RPTHEADER><RPTHEADER name="Foot LIST_FACILITY" version="1" keeptogether="1" keepgroup="0" repeat="0" forcefootpage="0"><RPTSECTION name="FootLIST_FACILITYSection"/></RPTHEADER><RPTHEADER name="Foot Page" version="1" keeptogether="1" keepgroup="0" repeat="0" forcefootpage="0"><RPTSECTION name="FootPageSection"/></RPTHEADER><RPTHEADER name="Foot Report" version="1" keeptogether="1" keepgroup="0" repeat="0" forcefootpage="0"><RPTSECTION name="FootReportSection"/></RPTHEADER></RPTHEADERS></REPORTLAYOUT>
