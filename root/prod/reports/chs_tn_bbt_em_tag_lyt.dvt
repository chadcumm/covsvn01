	, TAGLIST_PRODUCT_NBR = TAG_REQUEST->taglist[D.SEQ].product_nbr
	, TAGLIST_PRODUCT_SUB_NBR = TAG_REQUEST->taglist[D.SEQ].product_sub_nbr
	, TAGLIST_ALTERNATE_NBR = TAG_REQUEST->taglist[D.SEQ].alternate_nbr
	, TAGLIST_CUR_ABO_CD = TAG_REQUEST->taglist[D.SEQ].cur_abo_cd
	, TAGLIST_CUR_ABO_DISP = TAG_REQUEST->taglist[D.SEQ].cur_abo_disp
	, TAGLIST_CUR_RH_CD = TAG_REQUEST->taglist[D.SEQ].cur_rh_cd
