	, TAGLIST_ALPHA_TRANSLATION_IND = TAG_REQUEST->taglist[D.SEQ].alpha_translation_ind
	, TAGLIST_ALIAS_MRN_FORMATTED = TAG_REQUEST->taglist[D.SEQ].alias_mrn_formatted
	, TAGLIST_ALIAS_FIN_FORMATTED = TAG_REQUEST->taglist[D.SEQ].alias_fin_formatted
	, TAGLIST_ALIAS_SSN_FORMATTED = TAG_REQUEST->taglist[D.SEQ].alias_ssn_formatted
	, TAGLIST_FLAG_CHARS = TAG_REQUEST->taglist[D.SEQ].flag_chars
	, OWNER_MEDICAL_DIRECTOR = trim(ce.field_value)
	, OWNER_AREA_CD = PE.CUR_OWNER_AREA_CD
	, OWNER_AREA_DISP = UAR_GET_CODE_DISPLAY(PE.CUR_OWNER_AREA_CD)
	, PE_CUR_INV_LOCN_CD = PE.CUR_INV_LOCN_CD
	, PE_ORIG_INV_LOCN_CD = PE.ORIG_INV_LOCN_CD
	, TAG_REQUEST_TAGLIST_CMPNT_CNT = TAG_REQUEST->taglist[D.SEQ].cmpnt_cnt
	, PE_CUR_INV_LOCN_DISP = UAR_GET_CODE_DISPLAY(PE.CUR_INV_LOCN_CD)
	, PE_ORIG_INV_LOCN_DISP = UAR_GET_CODE_DISPLAY(PE.ORIG_INV_LOCN_CD)
	, PE_CUR_INV_AREA_CD = PE.CUR_INV_AREA_CD
	, PE_CUR_INV_AREA_DISP = UAR_GET_CODE_DISPLAY(PE.CUR_INV_AREA_CD)
	, A_STATE = UAR_GET_CODE_DISPLAY(A.STATE_CD)
	, A_STATE_CD = A.STATE_CD
	, A_CITY = A.CITY
	, A_ZIP = A.ZIPCODE
	, A_STREET1 = A.STREET_ADDR
	, A_STREET2 = A.STREET_ADDR2
	, A_STREET3 = A.STREET_ADDR3

FROM
	(DUMMYT   D  WITH SEQ = VALUE(SIZE(TAG_REQUEST->taglist, 5)))
	, PRODUCT   PE
	, CODE_VALUE_EXTENSION   CE
	, ADDRESS   A

PLAN D

 JOIN PE WHERE PE.PRODUCT_ID = TAG_REQUEST->TAGLIST[D.SEQ].PRODUCT_ID
 JOIN A WHERE PE.CUR_OWNER_AREA_CD = OUTERJOIN(A.PARENT_ENTITY_ID)          AND
              "LOCATION" = OUTERJOIN(A.PARENT_ENTITY_NAME)
 JOIN CE WHERE CE.CODE_VALUE = OUTERJOIN(PE.CUR_OWNER_AREA_CD)                          AND
               CE.FIELD_NAME = OUTERJOIN("BBT Medical Director")

WITH NOCOUNTER, SEPARATOR=" ", FORMAT, NULLREPORT]]></RPTQUERYSTRING><RPTSELECTIF condition="1"><![CDATA[]]></RPTSELECTIF><RPTSELECTIF condition="0"><![CDATA[]]></RPTSELECTIF><RPTSELECTIF condition="0"><![CDATA[]]></RPTSELECTIF></RPTQUERY><RPTRECORD><![CDATA[record tag_request (
