]]></RPTPROMPTS><RPTQUERY name="tag_query" layout="1"><RPTQUERYSTRING><![CDATA[SELECT
	TAGLIST_PRODUCT_EVENT_ID = TAG_REQUEST->taglist[D.SEQ].product_event_id
	, TAGLIST_EVENT_TYPE_CD = TAG_REQUEST->taglist[D.SEQ].event_type_cd
