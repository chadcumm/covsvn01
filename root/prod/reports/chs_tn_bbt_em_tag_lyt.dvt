declare 212_business        = f8 with protect, constant(uar_get_code_by_cki("CKI.CODEVALUE!8009"))
declare 333_attend_phys = f8 with public, constant(uar_get_code_by_cki ("CKI.CODEVALUE!4024"))
declare 333_order_phys  = f8 with public, constant(uar_get_code_by_cki ("CKI.CODEVALUE!9515"))
select into "nl:"
 from encntr_prsnl_reltn epr,
      prsnl p
 
 plan epr where expand(idx, 1, size(tag_request->taglist,5),
                        epr.encntr_id,
                        tag_request->taglist[idx].encntr_id)
                and epr.active_ind = 1
		        and epr.beg_effective_dt_Tm <= cnvtdatetime(curdate,curtime3)
		        and epr.end_effective_dt_tm >= cnvtdatetime(curdate,curtime3)
		        and epr.encntr_prsnl_r_cd in (333_attend_phys, 333_order_phys)
 
 join p where p.person_id = epr.prsnl_person_id
          and p.active_ind = 1
          and p.beg_effective_dt_tm <= cnvtdatetime(curdate,curtime3)
          and p.end_effective_dt_tm >= cnvtdatetime(curdate,curtime3)
 
 order by epr.encntr_id,epr.encntr_prsnl_r_cd
 
 head report
    encntrIdx = 0
 
 head epr.encntr_id
    encntrIdx = encntrIdx + 1
    stat = alterlist(encntr_info->encntr_list, encntrIdx)
    encntr_info->encntr_list[encntrIdx].encntr_id = epr.encntr_id
 
 head epr.encntr_prsnl_r_cd
    if (epr.encntr_prsnl_r_cd = 333_attend_phys)
        encntr_info->encntr_list[encntrIdx].attending_phys    = p.name_full_formatted
        encntr_info->encntr_list[encntrIdx].attending_phys_id = epr.prsnl_person_id
    elseif (epr.encntr_prsnl_r_cd = 333_order_phys)
        encntr_info->encntr_list[encntrIdx].ordering_phys     = p.name_full_formatted
        encntr_info->encntr_list[encntrIdx].ordering_phys_id  = epr.prsnl_person_id
    endif
 
 foot epr.encntr_prsnl_r_cd
    eCnt = encntrIdx
    null
 
 foot report
    null
 
 with nocounter, expand = 1
 
