]]></RPTPROMPTS><RPTQUERY name="Query1" layout="1"><RPTQUERYSTRING><![CDATA[SELECT DISTINCT
	Case_id = sc.surg_case_id
	, PAT_NAME = p.name_full_formatted
	, Surg_case_NBR = sc.surg_case_nbr_formatted
	, CASE_START = FORMAT(SC.SCHED_START_DT_TM, "MM-DD-YYYY;;D")
	 ; ,sc.surg_area_cd
	, fac_area = uar_get_code_display(sc.surg_area_cd)
	, anes_room = uar_get_code_display(cnvtreal(sca.case_attribute_value_txt))
	, SURG_ROOM = UAR_GET_CODE_DISPLAY(SC.SURG_OP_LOC_CD)
	, or_code = sc.surg_op_loc_cd
	, anes_code = cnvtreal(sca.case_attribute_value_txt)

FROM
	surgical_case   sc
	, sa_anesthesia_record   sar
	, sa_case_attribute   sca
	, person   p

plan sc  WHERE (sc.sched_start_dt_tm between cnvtdatetime($STARTDATE_PMPT) and cnvtdatetime($ENDDATE_PMPT))
         and sc.surg_area_cd =  $FACILITY_PMPT 
         and sc.active_ind = 1


join sar  where sar.surgical_case_id = sc.surg_case_id
          and sar.active_ind = 1
 
join sca where sca.sa_anesthesia_record_id = sar.sa_anesthesia_record_id
         and sca.case_attribute_type_cd = 4056668 ; code for anesthesia room in sca
         and sca.active_ind = 1
         
JOIN p   WHERE  p.person_id = sc.person_id

ORDER BY
	fac_area
	, case_start
	, surg_room
	, surg_case_nbr

WITH MAXREC = 3000, NOCOUNTER, SEPARATOR=" ", FORMAT, time=30]]></RPTQUERYSTRING><RPTSELECTIF condition="1"><![CDATA[]]></RPTSELECTIF><RPTSELECTIF condition="0"><![CDATA[]]></RPTSELECTIF><RPTSELECTIF condition="0"><![CDATA[]]></RPTSELECTIF></RPTQUERY><RPTHEADERS><RPTHEADER name="Head Report" version="1" keeptogether="1" keepgroup="0" repeat="0" forcefootpage="0"><RPTSECTION name="HeadReportSection"/></RPTHEADER><RPTHEADER name="Head Page" version="1" keeptogether="1" keepgroup="0" repeat="0" forcefootpage="0"><RPTSECTION name="HeadPageSection"/></RPTHEADER><RPTHEADER name="Head case_start" version="1" keeptogether="1" keepgroup="0" repeat="0" forcefootpage="0"><RPTSECTION name="Headcase_startSection"/></RPTHEADER><RPTHEADER name="Detail" version="1" keeptogether="1" keepgroup="0" repeat="0" forcefootpage="0"/><RPTHEADER name="Foot surg_case_nbr" version="1" keeptogether="1" keepgroup="0" repeat="0" forcefootpage="0"><RPTSECTION name="Footsurg_case_nbrSection"/></RPTHEADER><RPTHEADER name="Foot surg_room" version="1" keeptogether="1" keepgroup="0" repeat="0" forcefootpage="0"/></RPTHEADERS></REPORTLAYOUT>
