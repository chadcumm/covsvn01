]]></RPTPROMPTS><RPTQUERY name="get_apptList" layout="1"><RPTQUERYSTRING><![CDATA[SELECT
	PLIST_CLINIC = SUBSTRING(1, 30, APPT->plist[D1.SEQ].clinic)
	, PLIST_PAT_NAME = SUBSTRING(1, 30, APPT->plist[D1.SEQ].pat_name)
	, PLIST_PAT_DOB = APPT->plist[D1.SEQ].pat_dob
	, PLIST_PAT_MRN = SUBSTRING(1, 30, APPT->plist[D1.SEQ].pat_mrn)
	, PLIST_APP_TYPE = SUBSTRING(1, 30, APPT->plist[D1.SEQ].app_type)
	, PLIST_APP_DT = APPT->plist[D1.SEQ].app_dt
	, PLIST_PRIMARY_MD = SUBSTRING(1, 30, APPT->plist[D1.SEQ].primary_md)
	, PLIST_PAT_COMMENT = SUBSTRING(1, 200, APPT->plist[D1.SEQ].pat_comment)

FROM
	(DUMMYT   D1  WITH SEQ = VALUE(SIZE(APPT->plist, 5)))

PLAN D1

ORDER BY
	PLIST_CLINIC
	, PLIST_PAT_MRN
	, PLIST_APP_DT

WITH NOCOUNTER, SEPARATOR=" ", FORMAT]]></RPTQUERYSTRING><RPTSELECTIF condition="1"><![CDATA[]]></RPTSELECTIF><RPTSELECTIF condition="0"><![CDATA[]]></RPTSELECTIF><RPTSELECTIF condition="0"><![CDATA[]]></RPTSELECTIF></RPTQUERY><RPTDRIVER name="cov_cpm_ChartAbstraction" params="&quot;NL:&quot;,  $start_datetime, $end_datetime, $clinic_list"/><RPTRECORD><![CDATA[record appt (
