]]></RPTPROMPTS><RPTQUERY name="get_patients" layout="1"><RPTQUERYSTRING><![CDATA[SELECT
	LIST_FACILITY = EXPIRED_LOG->list[D1.SEQ].facility
	, LIST_FACILITY_NAME = SUBSTRING(1, 100, EXPIRED_LOG->list[D1.SEQ].facility_name)
	, ULIST_NURSE_UNIT_CD = EXPIRED_LOG->list[D1.SEQ].ulist[D2.SEQ].nurse_unit_cd
	, ULIST_NURSE_UNIT_NAME = SUBSTRING(1, 30, EXPIRED_LOG->list[D1.SEQ].ulist[D2.SEQ].nurse_unit_name)
	, PLIST_PERSON_ID = EXPIRED_LOG->list[D1.SEQ].ulist[D2.SEQ].plist[D3.SEQ].person_id
	, PLIST_ENCOUNTER_ID = EXPIRED_LOG->list[D1.SEQ].ulist[D2.SEQ].plist[D3.SEQ].encounter_id
	, PLIST_NAME = SUBSTRING(1, 50, EXPIRED_LOG->list[D1.SEQ].ulist[D2.SEQ].plist[D3.SEQ].name)
	, PLIST_MRN = SUBSTRING(1, 30, EXPIRED_LOG->list[D1.SEQ].ulist[D2.SEQ].plist[D3.SEQ].mrn)
	, PLIST_FIN = SUBSTRING(1, 30, EXPIRED_LOG->list[D1.SEQ].ulist[D2.SEQ].plist[D3.SEQ].fin)
	, PLIST_DOB = SUBSTRING(1, 30, EXPIRED_LOG->list[D1.SEQ].ulist[D2.SEQ].plist[D3.SEQ].dob)
	, PLIST_ADMIT_DATE = SUBSTRING(1, 30, EXPIRED_LOG->list[D1.SEQ].ulist[D2.SEQ].plist[D3.SEQ].admit_date)
	, PLIST_DISH_DATE = SUBSTRING(1, 30, EXPIRED_LOG->list[D1.SEQ].ulist[D2.SEQ].plist[D3.SEQ].dish_date)
	, PLIST_ADMIT_DX = SUBSTRING(1, 100, EXPIRED_LOG->list[D1.SEQ].ulist[D2.SEQ].plist[D3.SEQ].admit_dx)
	, PLIST_STATUS = SUBSTRING(1, 30, EXPIRED_LOG->list[D1.SEQ].ulist[D2.SEQ].plist[D3.SEQ].status)
	, PLIST_EXPIRE_DATE = SUBSTRING(1, 30, EXPIRED_LOG->list[D1.SEQ].ulist[D2.SEQ].plist[D3.SEQ].expire_date)
	, PLIST_DEATH_CHART_DT = SUBSTRING(1, 30, EXPIRED_LOG->list[D1.SEQ].ulist[D2.SEQ].plist[D3.SEQ].death_chart_dt)
	, PLIST_LAST_REST_CHART = SUBSTRING(1, 30, EXPIRED_LOG->list[D1.SEQ].ulist[D2.SEQ].plist[D3.SEQ].last_rest_chart)
	, PLIST_HRS_24 = SUBSTRING(1, 30, EXPIRED_LOG->list[D1.SEQ].ulist[D2.SEQ].plist[D3.SEQ].hrs_24)
	, PLIST_ONE_WEEK = SUBSTRING(1, 30, EXPIRED_LOG->list[D1.SEQ].ulist[D2.SEQ].plist[D3.SEQ].one_week)
	, PLIST_MD_ATTEND = SUBSTRING(1, 50, EXPIRED_LOG->list[D1.SEQ].ulist[D2.SEQ].plist[D3.SEQ].md_attend)
	, PLIST_MD_ADMIT = SUBSTRING(1, 50, EXPIRED_LOG->list[D1.SEQ].ulist[D2.SEQ].plist[D3.SEQ].md_admit)

FROM
	(DUMMYT   D1  WITH SEQ = VALUE(SIZE(EXPIRED_LOG->list, 5)))
	, (DUMMYT   D2  WITH SEQ = 1)
	, (DUMMYT   D3  WITH SEQ = 1)

PLAN D1 WHERE MAXREC(D2, SIZE(EXPIRED_LOG->list[D1.SEQ].ulist, 5))
JOIN D2 WHERE MAXREC(D3, SIZE(EXPIRED_LOG->list[D1.SEQ].ulist[D2.SEQ].plist, 5))
JOIN D3

ORDER BY
	LIST_FACILITY
	, ULIST_NURSE_UNIT_NAME
	, PLIST_NAME

WITH NOCOUNTER, SEPARATOR=" ", FORMAT,  NULLREPORT]]></RPTQUERYSTRING><RPTSELECTIF condition="1"><![CDATA[]]></RPTSELECTIF><RPTSELECTIF condition="0"><![CDATA[]]></RPTSELECTIF><RPTSELECTIF condition="0"><![CDATA[]]></RPTSELECTIF></RPTQUERY><RPTDRIVER name="cov_phq_restraint_expired_pat" params="&quot;NL:&quot;,  $start_datetime, $end_datetime, $facility_list" executionFlowOrder="1"/><RPTRECORD executionFlowOrder="0"><![CDATA[record expired_log (
  1 list [*]  
    2 facility = f8   
    2 facility_name = vc   
    2 ulist [*]  
      3 nurse_unit_cd = f8   
      3 nurse_unit_name = vc   
      3 plist [*]  
        4 person_id = f8   
        4 encounter_id = f8   
        4 name = vc   
        4 mrn = vc   
        4 fin = vc   
        4 dob = vc   
        4 admit_date = vc   
        4 dish_date = vc   
        4 admit_dx = vc   
        4 status = vc   
        4 expire_date = vc   
        4 death_chart_dt = vc   
        4 last_rest_chart = vc   
        4 hrs_24 = vc   
        4 one_week = vc   
        4 md_attend = vc   
