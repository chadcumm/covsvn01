]]></RPTPROMPTS><RPTQUERY name="get_patients" layout="1"><RPTQUERYSTRING><![CDATA[SELECT
	AUDIT_RECCNT = AUDIT->reccnt
	, AUDIT_LOCATION = SUBSTRING(1, 30, AUDIT->location)
	, PLIST_PROVIDER_ID = AUDIT->plist[D1.SEQ].provider_id
	, PLIST_PROVIDER_NAME = SUBSTRING(1, 30, AUDIT->plist[D1.SEQ].provider_name)
	, PLIST_PATID = AUDIT->plist[D1.SEQ].patid
	, PLIST_ORDER_DT = SUBSTRING(1, 30, AUDIT->plist[D1.SEQ].order_dt)
	, PLIST_DIFF = AUDIT->plist[D1.SEQ].diff
	, PLIST_PATID_90DAYS = AUDIT->plist[D1.SEQ].patid_90days

FROM
	(DUMMYT   D1  WITH SEQ = VALUE(SIZE(AUDIT->plist, 5)))

PLAN D1

ORDER BY
	AUDIT_LOCATION
	, PLIST_PROVIDER_NAME

WITH NOCOUNTER, SEPARATOR=" ", FORMAT]]></RPTQUERYSTRING><RPTSELECTIF condition="1"><![CDATA[]]></RPTSELECTIF><RPTSELECTIF condition="0"><![CDATA[]]></RPTSELECTIF><RPTSELECTIF condition="0"><![CDATA[]]></RPTSELECTIF></RPTQUERY><RPTDRIVER name="cov_phq_contrld_substa_audit" params="'NL:', $location,$start_datetime,$end_datetime"/><RPTRECORD><![CDATA[record audit (
  1 reccnt = i4   
  1 location = vc   
  1 plist [*]  
    2 provider_id = f8   
    2 provider_name = vc   
    2 patid = f8   
    2 order_dt = vc   
    2 diff = f8   
