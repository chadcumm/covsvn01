]]></RPTPROMPTS><RPTQUERY name="items" layout="1"><RPTQUERYSTRING><![CDATA[SELECT
	CASE_START = FORMAT(SC.SCHED_START_DT_TM, "MM-DD-YYYY;;D")
	, FIN = ea.alias
	, PAT_NAME = p.name_full_formatted
	, SCH_PROC = UAR_GET_CODE_DISPLAY(scp.surg_proc_cd)
	, PRIMSURG = PE2.NAME_FULL_FORMATTED
	, FREE_TEXT_DESC = cp.free_text_item_desc
	, PICKLIST_ID = cp.case_cart_pick_list_id
	, MOD_DATE = FORMAT(cp.updt_dt_tm, "MM-DD-YYYY;;D")
	, WHO_MOD = pe4.name_full_formatted
	, Qty = cp.qty_used

FROM
	surgical_case   sc
	, encounter   en
	, surg_case_procedure   scp
	, person   p
	, prsnl   pe2
	, prsnl   pe4
	, encntr_alias   ea
	, case_cart_pick_list   cp

PLAN sc
            WHERE (sc.sched_start_dt_tm between cnvtdatetime($STARTDATE_PMPT) and cnvtdatetime($ENDDATE_PMPT))
            AND sc.surg_area_cd = CNVTINT($FACILITY_PMPT)
            and sc.active_ind = 1
 

JOIN en   WHERE en.encntr_id = sc.encntr_id 
   		  and en.active_ind = 1
		  and en.end_effective_dt_tm >= sysdate
 
JOIN scp
            WHERE scp.surg_case_id = sc.surg_case_id
            AND scp.active_ind = 1
 
JOIN p 
	WHERE  p.person_id = sc.person_id

 
JOIN pe2
            WHERE pe2.person_id = scp.primary_surgeon_id
 
JOIN ea     WHERE ea.encntr_id = sc.encntr_id
            AND ea.encntr_alias_type_cd = 1077
 
JOIN cp
            WHERE cp.surg_case_id = sc.surg_case_id
            and cp.item_id = 0.0
            and cp.qty_used > 0
 
JOIN pe4
            WHERE pe4.person_id = cp.updt_id

ORDER BY
	case_start
	, PAT_NAME
	, PICKLIST_ID

WITH MAXREC = 3000, NOCOUNTER, SEPARATOR=" ", FORMAT, time=30, NULLREPORT]]></RPTQUERYSTRING><RPTSELECTIF condition="1"><![CDATA[]]></RPTSELECTIF><RPTSELECTIF condition="0"><![CDATA[]]></RPTSELECTIF><RPTSELECTIF condition="0"><![CDATA[]]></RPTSELECTIF></RPTQUERY><RPTQUERY name="noresults" layout="0"><RPTQUERYSTRING><![CDATA[SELECT INTO "NL:"
FROM
	DUMMYT   D1

