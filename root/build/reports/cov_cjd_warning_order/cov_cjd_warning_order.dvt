]]></RPTPROMPTS><RPTQUERY name="Query1" layout="1"><RPTQUERYSTRING><![CDATA[SELECT INTO "NL:"
	o.order_id
	, accession = cnvtacc(a.accession)
	, o.order_mnemonic
	, e.encntr_id
	, ea.alias
	, p.name_full_formatted
	, o.orig_order_dt_tm ";;q"
	, location = concat(trim(uar_get_code_display(e.loc_nurse_unit_cd))," ",trim(uar_get_code_display(e.loc_room_cd)),
		"-",trim(uar_get_code_display(e.loc_room_cd)))
	, order_status = uar_get_code_display(o.order_status_cd)

FROM
	orders   o
	, accession_order_r   aor
	, accession   a
	, encounter   e
	, person   p
	, encntr_alias   ea

plan o
	where o.order_id = link_orderid
join aor
	where aor.order_id = o.order_id
join a
	where a.accession_id = aor.accession_id
join e
	where e.encntr_id = o.encntr_id
join p
	where p.person_id = e.person_id
join ea
	where ea.encntr_id = e.encntr_id
	and   ea.active_ind = 1
	and   ea.encntr_alias_type_cd = value(uar_get_code_by("MEANING",319,"FIN NBR"))
	and   ea.beg_effective_dt_tm <= cnvtdatetime(curdate,curtime3)
	and   ea.end_effective_dt_tm >= cnvtdatetime(curdate,curtime3)

ORDER BY
	o.order_id

WITH NOCOUNTER, SEPARATOR=" ", FORMAT]]></RPTQUERYSTRING><RPTSELECTIF condition="1"><![CDATA[]]></RPTSELECTIF><RPTSELECTIF condition="0"><![CDATA[]]></RPTSELECTIF><RPTSELECTIF condition="0"><![CDATA[]]></RPTSELECTIF></RPTQUERY><RPTPREPROCESS><![CDATA[if (validate(link_orderid) = 0)
