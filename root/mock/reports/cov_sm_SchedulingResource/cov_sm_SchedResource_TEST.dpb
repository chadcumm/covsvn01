<property name = 'SETTINGS:00'><![CDATA[DEFAULT-KEY='' QUERY-STRING='select
    so.sch_object_id
    , so.mnemonic
from SCH_OBJECT so 
where
    so.object_type_cd &#061; value(uar_get_code_by("MEANING", 16146, "QUEUE"))
    and so.mnemonic_key not in ("BH*", "PBH*", "AMB*")
    and so.mnemonic_key not in ("*IT*USE*ONLY*", "DISPLACE*")
    and so.active_ind &#061; 1
order by
    if (cnvtupper(so.mnemonic_key) in ("UNKNOWN*"))
        build("0", so.mnemonic)
    elseif (cnvtupper(so.mnemonic_key) in ("*CENTRALIZED*", "FSR*WEST*DIAGNOSTIC*CENTER", "CHDC*WEST*LOCATION"))
        build("1", so.mnemonic)
    elseif (cnvtupper(so.mnemonic_key) in ("*HOLD*"))
        build("2", so.mnemonic)
    else
        so.mnemonic
    endif
















